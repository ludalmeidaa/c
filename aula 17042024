#include <stdio.h>
#include <stdlib.h>

int main() {
  int cont = 0;
  int tab = 1, x = 0;

  printf("=====================\n");

  for (x = 0;  x < 10;) {
    for (cont = 0; cont < 10;) {
      printf("%d x %2d = %2d\n", tab + x, cont, (tab + x) * cont);
      cont = cont + 1;
    }
    printf("=====================\n");

    x++;
  }
  return 0;
}
